{
  "manifest_version": 2,
  "name": "Lasttica",
  "description": "Chome extension for scrobble music to last.fm",
  "version": "0.0.1",
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": [ "app.js" ]
    }
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "default-icon.png",
    "default_popup": "popup.html"
  },
  "icons": { "28": "last.png" }
}
